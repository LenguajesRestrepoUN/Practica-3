fin_funcion

funcion entero lol() hacer
    entero dig1,dig2,dig3;
    para ( dig1 = 0 ; dig1 <= 9; 1 ) hacer
        para ( dig2 = 0; dig2 <= 9; 1) hacer
            para (dig3=0; dig3 <= 9; 1) hacer
                si ( dig1*dig1 + dig2*dig2 + dig3*dig3 == dig1*dig1*dig1) entonces
                    imprimir( dig1*dig1*dig1 , "\n" );
                fin_si
            fin_para
        fin_para
    fin_para
    retornar 1;
fin_funcion

funcion entero resta() hacer
    entero a = 1;
    entero b = 2;
    cadena d= "\n";
    entero res;
    si ( a > b ) entonces
        res = a;
        booleano flag = verdadero;
        si ( a > b ) entonces
                entero res = a;
                booleano flag2 = verdadero;
                si ( a > b ) entonces
                                entero res = a;
                                booleano flag2 = verdadero;
                            si_no
                                booleano flag2 = falso;
                                entero res = b;
                fin_si
            si_no
                booleano flag2 = falso;
                entero res = b;
        fin_si
    si_no
        booleano flag = falso;
        res = b;
    fin_si
    retornar a + b;
fin_funcion

